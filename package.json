{
  "name": "@hasurify/lambda",
  "version": "0.0.1",
  "description": "Hasurify Lambda Library",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "dev": "tsc -w",
    "tsc": "tsc --build",
    "prebuild": "yarn clean",
    "build": "yarn tsc",
    "prepare": "yarn build",
    "lint": "eslint 'src/**/*.{js,ts,jsx,tsx}' --quiet --fix",
    "format": "prettier --ignore-path=.gitignore --loglevel=error --write ./src",
    "commit": "cz",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:hasurify/lambda.git"
  },
  "author": "KenStack Team",
  "license": "ISC",
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "@types/aws-lambda": "^8.10.109",
    "@types/cli-color": "^2.0.2",
    "@types/node-fetch": "^2.5.11",
    "@types/request-ip": "^0.0.37",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "aws-cdk": "2.54.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.1",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "semantic-release": "^21.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.7.4"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "npm run lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@middy/http-error-handler": "^4.0.9",
    "@middy/http-json-body-parser": "^4.2.7",
    "@slack/types": "^2.8.0",
    "@slack/webhook": "^6.1.0",
    "aws-cdk-lib": "2.54.0",
    "aws-lambda": "^1.0.7",
    "cli-color": "^2.0.3",
    "constructs": "^10.0.0",
    "middy": "^0.36.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "bugs": {
    "url": "https://github.com/hasurify/lambda/issues"
  },
  "homepage": "https://github.com/hasurify/lambda#readme"
}
